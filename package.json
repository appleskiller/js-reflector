{
  "name": "js-reflector",
  "version": "1.1.4",
  "description": "js-reflector for Typescript",
  "main": "dist/js-reflector.common.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "typescript",
    "javascript",
    "reflection"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:main",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "build:main": "node build/build.js",
    "test": "karma start",
    "test:release": "karma start --single-run true",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "npm run test:release && npm run changelog && bash build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appleskiller/js-reflector.git"
  },
  "author": "appleskiller",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.54",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-typescript": "^3.0.8",
    "mocha": "^4.0.1",
    "rollup": "^0.51.8",
    "rollup-plugin-replace": "^2.0.0",
    "typings": "^2.1.1",
    "uglify-js": "^3.1.10"
  }
}
